<div class="dinamic-view-container">
    {{#if users}}
    <h2>Capybaras found</h2>
    <div class="searchBar searchBar-mobile">
        <form action="/list" method="POST" class="d-flex" role="search">
            <input name="searchInfo" id="searchQueryInput" type="text" name="searchQueryInput"
                placeholder="Search name or username" />
            <button id="searchQuerySubmit" type="submit" name="searchQuerySubmit">
                <svg style="width:24px;height:24px" viewBox="0 0 24 24">
                    <path fill="#666666"
                        d="M9.5,3A6.5,6.5 0 0,1 16,9.5C16,11.11 15.41,12.59 14.44,13.73L14.71,14H15.5L20.5,19L19,20.5L14,15.5V14.71L13.73,14.44C12.59,15.41 11.11,16 9.5,16A6.5,6.5 0 0,1 3,9.5A6.5,6.5 0 0,1 9.5,3M9.5,5C7,5 5,7 5,9.5C5,12 7,14 9.5,14C12,14 14,12 14,9.5C14,7 12,5 9.5,5Z" />
                </svg>
            </button>
        </form>
    </div>
    <div class="search-list-container">
        {{#each users as |user|}}
        <a href="/profile/{{user.username}}">
            <div class="seach-user-container">
                <img src="{{user.image}}" class="image-cover-medium">
                <div>
                    <p class="search-name">{{user.name}}</p>
                    <p class="search-username">{{user.username}}</p>
                </div>
            </div>
        </a>
        {{/each}}
    </div>
    {{else}}
    <h2>No Capybaras found &#128528</h2>
    {{/if}}
</div>